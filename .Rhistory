library(ggplot2)
library(dslabs)
library(dplyr)
library(tidyverse)
library(ggstar)
summary(stars)
ggplot(stars, aes(magnitude, temp, color = type)) +
geom_star(aes(size = -magnitude), starshape = 24) +
coord_radial(start = -0.5 * pi, end = 0.5 * pi, inner.radius = 0.1) +
labs(title = "Temperature and Magnitude of Stars",
x = "Star Temperature (Kelvin)",
y = "Magnitude of Star (Luminosity)") +
scale_color_manual(values = c(
"M" = "red",
"K" = "orange",
"G" = "yellow",
"F" = "lightgray",
"A" = "white",
"B" = "lightblue",
"O" = "blue",
"DA" = "purple",
"DB" = "magenta",
"DF" = "green"
),
name = "Star Type",
labels = c("M" = "M - Red Stars",
"K" = "K - Orange Stars",
"G" = "G - Yellow Stars",
"F" = "F - Yellow-White Stars",
"A" = "A - White Stars",
"B" = "B - Blue-White Stars",
"O" = "O - Blue Stars",
"DA" = "DA - White Dwarf",
"DB" = "DB - White Dwarf",
"DF" = "DF - White Dwarf")
) +
scale_size_continuous(range = c(1, 5), guide = "none") +
theme_dark() +
theme(legend.position = "bottom",
legend.spacing = unit(0.2, "cm"),
legend.key.size = unit(0.5, "cm"),
legend.text = element_text(size = 8),
legend.title = element_text(size = 6),
legend.box.background = element_blank(),
panel.background = element_rect(fill = "black"),
panel.grid.major = element_line(color = "white", size = 0.5),
panel.grid.minor = element_line(color = "white", size = 0.3))
stars %>%
group_by(type) %>%
summarize(lowest_magnitude = min(magnitude),
highest_magnitude = max(magnitude),
avg_magnitude = mean(magnitude),
avg_temp = mean(temp),
min_temp = min(temp),
star_count = n()) %>%
ungroup() %>%
mutate(type = factor(type, levels = type[order(avg_magnitude)]))
