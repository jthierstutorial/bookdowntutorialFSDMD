names(custom_labels) <- stars_summary$type
stars <- stars %>%
mutate(type = factor(type, levels = levels(stars_summary$type)))
ggplot(stars, aes(magnitude, temp, color = type)) +
coord_radial(start = -0.5 * pi, end = 0.5 * pi, inner.radius = 0.1, expand = TRUE, rotate.angle = TRUE) +
geom_star(starshape = 24, size = 2, position = "jitter") +
facet_wrap(~ type, labeller = as_labeller(custom_labels)) +
geom_vline(data = stars_summary, aes(xintercept = avg_magnitude),
linetype = "dashed", color = "black", size = 0.8) +
geom_text(data = stars_summary, aes(x = avg_magnitude, y = max(avg_temp) * 1.05,
label = paste("Avg Mag:", round(avg_magnitude, 2))),
color = "black", hjust = 0.5, vjust = -1.5, size = 3) +
geom_hline(data = stars_summary, aes(yintercept = avg_temp),
linetype = "dashed", color = "black", size = 0.8) +
geom_text(data = stars_summary, aes(x = Inf, y = avg_temp * 0.9,
label = paste("Avg Temp:", round(avg_temp, 2))),
color = "black", angle = 90, hjust = 0.5, vjust = 2, size = 3) +
labs(title = "Temperature and Magnitude of Stars by Type",
subtitle = "Note: Magnitude is a measure of brightness on a logarithmic scale - lower values equate to brighter stars",
x = "Magnitude of Star (Luminosity)",
y = "Star Surface Temperature (Kelvin)") +
scale_color_manual(values = c(
"M" = "red",
"K" = "orange",
"G" = "yellow",
"F" = "gold",
"A" = "blue",
"B" = "cyan",
"O" = "turquoise",
"DA" = "purple",
"DB" = "hotpink",
"DF" = "green"
)) +
theme_dark() +
theme(legend.position = "none",
strip.background = element_rect(fill = "black", color = "white"),
strip.text = element_text(color = "white", size = 10),
panel.background = element_rect(fill = "grey"),
panel.grid.major = element_line(color = "white", size = 0.5),
panel.grid.minor = element_line(color = "white", size = 0.3))
